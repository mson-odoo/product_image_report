<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_move_form_inherit_barcode_so_line" model="ir.ui.view">
        <field name="name">view.move.form.inherit.barcode.so.line</field>
        <field name="model">account.move</field>
        <field name="inherit_id" ref="account.view_move_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='invoice_line_ids']/tree/field[@name='product_id']" position="after">
                <field name="image_1920" widget="image" style="height:40px; width:40px;" options='{"zoom": true}'/>   
            </xpath>
        </field>
    </record>
</odoo>
